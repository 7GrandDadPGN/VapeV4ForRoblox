						for i,v in pairs(tempblocks) do
							spidermessage.Text = "Parts Processed : "..tostring(i).."/"..tostring(#tempblocks)
							if i % 100 == 0 then
								wait(0.4)
							end
							for i3 = 1, v.Size.Z + 1 do
								for i2 = 1, v.Size.X + 1 do
									createpart(Vector3.new(i2, v.Size.Y / 2, i3), v.Size.Y, v)
