								end
							end)
						end
					end
					if getEquipped()["Type"] ~= "bow" then
						targetinfo.UpdateInfo(targettable, targetsize)
					end
				end
			end)
		else
			killauranear = false
			bedwars["ViewmodelController"]["playAnimation"] = oldplay
			bedwars["SoundManager"]["playSound"] = oldsound
			oldplay = nil
			pcall(function()
