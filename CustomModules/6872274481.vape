	local function updatecolor(val)
		local newtext = ""
		local newfirst = false
		for i2,v2 in pairs(textwithoutthing:split("\n")) do
			local rainbowsub = 2
			local rainbowcolor = val + (WinstreakTrackerColor["RainbowValue"] and (-0.015 * i2) or 0)
